{
    "version": 2,
    "builds": [
      {
        "src": "next.config.ts",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/_next/(.*)",
        "dest": "/_next/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "rewrites": [],
    "redirects": [],
    "headers": []
  }
  